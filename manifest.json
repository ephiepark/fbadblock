{
    "manifest_version": 2,

    "name": "CommentOut",
    "description": "Blocks ads in fb comments",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "test.html"
    },
    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/"
    ]
}
